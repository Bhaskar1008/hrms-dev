    "forms": {
      "employee_create": {
        "id": "employee_create",
        "title": "Add New Employee",
        "description": "Create a new employee record in the system",
        "submitUrl": "/api/employees",
        "method": "POST",
        "fields": [
          {
            "id": "firstName",
            "type": "text",
            "label": "First Name",
            "placeholder": "Enter first name",
            "required": true,
            "validation": {
              "minLength": 2,
              "maxLength": 50
            }
          },
          {
            "id": "lastName",
            "type": "text",
            "label": "Last Name",
            "placeholder": "Enter last name",
            "required": true,
            "validation": {
              "minLength": 2,
              "maxLength": 50
            }
          },
          {
            "id": "email",
            "type": "email",
            "label": "Email",
            "placeholder": "Enter email address",
            "required": true,
            "validation": {
              "pattern": "^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,}$i",
              "message": "Invalid email address"
            }
          },
          {
            "id": "phone",
            "type": "tel",
            "label": "Phone",
            "placeholder": "Enter phone number",
            "required": false
          },
          {
            "id": "departmentId",
            "type": "select",
            "label": "Department",
            "required": true,
            "options": [
              {
                "label": "Engineering",
                "value": "1"
              },
              {
                "label": "Marketing",
                "value": "2"
              },
              {
                "label": "Finance",
                "value": "3"
              }
            ]
          },
          {
            "id": "jobTitle",
            "type": "text",
            "label": "Job Title",
            "placeholder": "Enter job title",
            "required": true
          },
          {
            "id": "hireDate",
            "type": "date",
            "label": "Hire Date",
            "required": true
          },
          {
            "id": "salary",
            "type": "number",
            "label": "Salary",
            "placeholder": "Enter annual salary",
            "required": true,
            "validation": {
              "min": 0
            }
          },
          {
            "id": "managerId",
            "type": "select",
            "label": "Manager",
            "required": false,
            "options": [
              {
                "label": "HR Manager",
                "value": "2"
              }
            ]
          },
          {
            "id": "status",
            "type": "select",
            "label": "Status",
            "required": true,
            "options": [
              {
                "label": "Active",
                "value": "active"
              },
              {
                "label": "Probation",
                "value": "probation"
              },
              {
                "label": "Inactive",
                "value": "inactive"
              }
            ],
            "defaultValue": "active"
          }
        ]
      },
      "leave_request": {
        "id": "leave_request",
        "title": "Submit Leave Request",
        "description": "Request time off from work",
        "submitUrl": "/api/leaves",
        "method": "POST",
        "fields": [
          {
            "id": "leaveTypeId",
            "type": "select",
            "label": "Leave Type",
            "required": true,
            "options": [
              {
                "label": "Annual Leave",
                "value": "1"
              },
              {
                "label": "Sick Leave",
                "value": "2"
              },
              {
                "label": "Maternity/Paternity Leave",
                "value": "3"
              }
            ]
          },
          {
            "id": "startDate",
            "type": "date",
            "label": "Start Date",
            "required": true
          },
          {
            "id": "endDate",
            "type": "date",
            "label": "End Date",
            "required": true,
            "validation": {
              "min": "startDate"
            }
          },
          {
            "id": "reason",
            "type": "textarea",
            "label": "Reason",
            "placeholder": "Explain the reason for your leave request",
            "required": true,
            "validation": {
              "minLength": 10,
              "maxLength": 500
            }
          },
          {
            "id": "attachment",
            "type": "file",
            "label": "Supporting Document",
            "required": false,
            "fileTypes": ".pdf,.jpg,.png",
            "maxSize": 5242880
          }
        ]
      }
    },
